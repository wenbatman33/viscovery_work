<!DOCTYPE html>
<html lang="en" data-page="index">
<head>
  <meta charset="UTF-8">
  <base href="<%= htmlWebpackPlugin.options.base %>">
  <meta name="viewport" content="width=device-width, user-scalable=0, initial-scale=1">
  <%= require("../../meta.ejs")() %>
  <title>Viscovery</title>
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <%= require("../commons/vendorJS.ejs")() %>
  <%= require("../../ga.ejs")() %>
</head>
<body class="lang-en">
  <%= require("../commons/header.ejs")() %>
  <div class='visc-main index-main'>

    <!-- Hero Block -->
    <div class="hero-block hero-block__no-msk">
      <div id="waves" class="index-main__waves"></div>
      <div class="recognition" id="recognition-area">
        <canvas id="recognition" class="recognition__man"></canvas>
      </div>
      <div class="recognition__info">
        <div class="container-med recognition__area">
          <div class="recognition__description">
            <h3 class="recognition__title">Video AI Discovery, Unlimited Possibilities</h3>
            <p class="recognition__sub ff-Gotham-Reglular">Visual Search, Simply Smarter</p>
          </div>
        </div>
      </div>
    </div>


    <!-- Round Plan -->
    <div class="round-plan">
      <div class="container-med">
        <div class="round-plan__title">
          <img class="round-plan__title__logo" src="imgs/round-plan/VDS.svg" />
          <h3 class="color-white round-plan__title__text">Video Discovery Service<br/>5 Major Solutions</h3>
        </div>
      </div>

      <div class="round-plan__box">
        <div class="round-plan__container">
          <div class="round-plan__plan">
            <h4 class="round-plan__plan-text round-plan__plan-text--position-1">Video Search<br/>Engine</h4>
            <h4 class="round-plan__plan-text round-plan__plan-text--position-2">IoT<br/>Solution</h4>
            <h4 class="round-plan__plan-text round-plan__plan-text--position-3">Customized<br/>Service</h4>
            <h4 class="round-plan__plan-text round-plan__plan-text--position-4">Video Content<br/>Interaction</h4>
            <h4 class="round-plan__plan-text round-plan__plan-text--position-5">Contextual<br/>Advertisement</h4>

            <div class="round-plan__tip round-plan__tip--position-1">
              <img class="tip-line-1" src="imgs/round-plan/tip-line-1.svg" />
              <p class="tip-title">V-SEARCH</p>
              <p class="tip-description">Search key moments in videos<br/>Significantly increase the efficiency and accuracy of searching</p>
            </div>
            <div class="round-plan__tip round-plan__tip--position-2">
              <img class="tip-line-2" src="imgs/round-plan/tip-line-2.svg" />
              <p class="tip-title">IoT</p>
              <p class="tip-description">Consumer behavior recognition and big data analysis<br/>360° Application: Smart household / Smart Retail</p>
            </div>
            <div class="round-plan__tip round-plan__tip--position-3">
              <img class="tip-line-3" src="imgs/round-plan/tip-line-3.svg" />
              <p class="tip-title">Customized Service</p>
              <p class="tip-description">Customize video<br/>solution services</p>
            </div>
            <div class="round-plan__tip round-plan__tip--position-4">
              <img class="tip-line-4" src="imgs/round-plan/tip-line-4.svg" />
              <p class="tip-title">V-BUY</p>
              <!-- <p class="tip-description">Multi-Screen Interaction<br/>Video Content Interpretation<br/>Consumer Behavior Analysis<br/>Accurate Product Recommendation<br/>Video Content Realization</p> -->
              <p class="tip-description">Video content interpretation<br/>Consumer behavior analysis<br/>Accurate product recommendation<br/>Video content monetization</p>
            </div>
            <div class="round-plan__tip round-plan__tip--position-5">
              <img class="tip-line-5" src="imgs/round-plan/tip-line-5.svg" />
              <p class="tip-title">KEY<br/>MOMENT</p>
              <p class="tip-description">Advertise at the right time<br/>to maximize effectiveness</p>
            </div>

          </div>
          <canvas id="round-plan" class="round-plan__canvas" height="540"></canvas>
        </div>
      </div>

      <!-- mobile tip -->
      <div class="round-plan__mobile font-h6 color-white">
        <span class="round-plan__mobile-tip"><p class="tip-1"><span class="color-green">V-SEARCH</span> Search key moments in videos; Significantly increase the efficiency and accuracy of searching</p></span>
        <span class="round-plan__mobile-tip"><p class="tip-2"><span class="color-green">IoT</span> Consumer behavior recognition and big data analysis; 360° Application: Smart household / Smart Retail</p></span>
        <span class="round-plan__mobile-tip"><p class="tip-3"><span class="color-green">Customized Service</span> Customize video solution services</p></span>
        <span class="round-plan__mobile-tip"><p class="tip-4"><span class="color-green">V-BUY</span> Video content interpretation<br/>Consumer behavior analysis<br/>Accurate product recommendation<br/>Video content monetization</p></span>
        <span class="round-plan__mobile-tip"><p class="tip-5"><span class="color-green">KEY MOMENT</span> Advertise at the right time to maximize effectiveness</p></span>

        <ol class="round-plan__dots-container">
          <li class="round-plan__dots"></li>
          <li class="round-plan__dots"></li>
          <li class="round-plan__dots"></li>
          <li class="round-plan__dots"></li>
          <li class="round-plan__dots"></li>
        </ol>
      </div>
    </div>


    <!-- Advantage Block -->
    <div class="adv-block">
      <h3 class="adv-block__title font-h1 color-dark">VDS Technology Highlights</h3>
      <%= require("../commons/intro-list.ejs")() %>
    </div>


    <!-- Partner Block -->
    <%= require("../commons/partner-list.ejs")() %>


  </div>
  <%= require("../commons/footer.ejs")() %>

  <script src="scripts/three-js/three.min.js"></script>
  <script src="scripts/three-js/renderers/Projector.js"></script>
  <script src="scripts/three-js/renderers/CanvasRenderer.js"></script>
  <script src="scripts/three-js/Detector.js"></script>
</body>
</html>
