    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "fetch-mock": "^5.12.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.12.0",
    "jsdom-global": "^2.1.1",
    "json-loader": "^0.5.4",
