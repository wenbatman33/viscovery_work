{
  "name": "vsp-frontend",
  "version": "1.6.2",
  "scripts": {
    "develop": "webpack-dashboard -- webpack-dev-server --config webpack.config.dev.js",
    "release": "yarn run lint && NODE_ENV=production webpack --config webpack.config.prod.js",
    "test": "BABEL_DISABLE_CACHE=1 NODE_ENV=test mocha --compilers js:babel-register -r jsdom-global/register -r ignore-styles --recursive src/js/**/*.test.js",
    "watch-test": "BABEL_DISABLE_CACHE=1 NODE_ENV=test mocha --compilers js:babel-register -r jsdom-global/register -r ignore-styles --watch --recursive src/js/**/*.test.js",
    "lint": "eslint src",
    "lint-staged": "lint-staged",
    "profile": "webpack --config webpack.config.prod.js --profile --json > stats.json"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.js": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "git@git-cn.viscovery.co:dreamcwli/vsp-frontend.git"
  },
